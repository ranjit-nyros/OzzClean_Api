  <div class="wrapper content">
   <div id="content-wrapper" class="clearfix row">
          <div class="span12 well"  style="height:100px;">
        <center><h2> OzzClean Help / API for Developers</h2></center>
        <hr > </hr>
        <center><h2>Request API Key</h2></center>
      </div>
      <div class="span12"> 
      
      <div class="span8">
<%= simple_form_for @api_key,:html => {method: :post, class: 'form-horizontal'} do |f| %>
  <%= f.error_notification %>

  <fieldset >
    <legend>Request Api Key </legend>
     <br/>
      <div class="form-inputs">
    
      
      <%= f.input :name  %>
   
      <%= f.input :email %>
      <%= f.input :company,:label => "Company Name"  %>
        
    </div>
  </fieldset>
  
 <div class="form-actions">
  <p><%= f.submit "Generate API Key" , class: 'btn btn-primary' %></p>
  </div>
<% end %>
    </div>

    </div>
    </div>
</div>